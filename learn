console.log(`Length: ${Object.keys(Routing).length}`);




++++JAVASCRIPT PROMISE++++
p = new Promise((resl, rejc)=>{
resl();   //If resl() is fulfilled...
rejc();   //then rejc() is ignored...
})

p.then(
(value)=>{console.log("success")}, //if value is found...
(error)=>{console.log("error")}    //error isn't present.
)

